<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<HBox id="AnchorPane" alignment="CENTER_RIGHT" prefHeight="120.0" spacing="50.0" style="-fx-border-width: 0 0 1px 0; -fx-border-color: grey;" stylesheets="@../assets/css/styles.css" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Button fx:id="delete" mnemonicParsing="false" prefHeight="46.0" prefWidth="50.0">
         <graphic>
            <FontAwesomeIconView glyphName="TRASH">
               <styleClass>
                  <String fx:value="btn" />
                  <String fx:value="btn-primary" />
               </styleClass>
            </FontAwesomeIconView>
         </graphic>
         <styleClass>
            <String fx:value="btn" />
            <String fx:value="btn-primary-outline" />
         </styleClass>
         <HBox.margin>
            <Insets left="25.0" right="25.0" />
         </HBox.margin>
      </Button>
      <ImageView fx:id="image" fitHeight="100.0" fitWidth="70.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="name" contentDisplay="CENTER" prefWidth="160.0" text="Label">
         <HBox.margin>
            <Insets left="25.0" />
         </HBox.margin>
      </Label>
      <Label fx:id="price" lineSpacing="10.0" text="Label" />
      <Label fx:id="qntt" lineSpacing="10.0" text="Label" />
      <Label fx:id="total" text="Label">
         <HBox.margin>
            <Insets right="50.0" />
         </HBox.margin>
      </Label>
   </children>
</HBox>
