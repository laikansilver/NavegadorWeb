<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="CENTER" stylesheets="@../assets/css/styles.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER">
         <children>
            <ImageView pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../assets/images/shape-left.png" />
               </image>
            </ImageView>
            <Label styleClass="section-title" text="Les Enseignants" HBox.hgrow="ALWAYS" />
            <ImageView pickOnBounds="true" preserveRatio="true" rotate="180.0">
               <image>
                  <Image url="@../assets/images/shape-left.png" />
               </image>
            </ImageView>
         </children>
      </HBox>
   </children>
</VBox>
