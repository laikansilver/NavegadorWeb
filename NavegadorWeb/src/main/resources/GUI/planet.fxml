<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="bgImage" pickOnBounds="true" preserveRatio="true" />
      <VBox fx:id="container" alignment="CENTER">
         <children>
            <FlowPane fx:id="imageContainer" alignment="CENTER" columnHalignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="260.0" prefWidth="260.0" style="-fx-border-color: black; -fx-border-style: dashed; -fx-border-width: 5px; -fx-border-radius: 100%;">
               <children>
                  <ImageView fx:id="roundImage" fitHeight="180.0" fitWidth="180.0" pickOnBounds="true" preserveRatio="true" />
               </children>
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </VBox.margin>
            </FlowPane>
            <Label fx:id="title" alignment="CENTER" contentDisplay="CENTER" graphicTextGap="10.0" style="-fx-font-family: 'Dosis', sans-serif; -fx-font-size: 34px;" text="Label" textAlignment="CENTER">
               <VBox.margin>
                  <Insets bottom="20.0" top="20.0" />
               </VBox.margin>
            </Label>
            <Label fx:id="subTitle" alignment="CENTER" style="-fx-font-size: 20px;" text="Label">
                <VBox.margin>
                    <Insets bottom="25.0" top="20.0" />
                </VBox.margin>
            </Label>
         </children>
      </VBox>
   </children>
</StackPane>
